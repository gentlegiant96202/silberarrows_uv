<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.silberarrows.portal-server</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Volumes/SilberArrows/CODEBASE/PORTAL NEW NEW/check-and-start-server.sh</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>StartOnMount</key>
    <true/>
    
    <key>StartInterval</key>
    <integer>30</integer>
    
    <key>StandardOutPath</key>
    <string>/tmp/portal-server-launchd.log</string>
    
    <key>StandardErrorPath</key>
    <string>/tmp/portal-server-launchd-error.log</string>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
    
    <key>KeepAlive</key>
    <false/>
</dict>
</plist>







